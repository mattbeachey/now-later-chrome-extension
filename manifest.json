
{
    "name": "Now and Later",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html",  
      "default_title": "Now & Later",
      "default_icon": "./assets/icon.png"
    },
    "background": {
      "page": "background.html"
    },
    "permissions": [
      "tabs",
      "identity"
    ],
    "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'; connect-src 'self' wss://*.firebaseio.com;",
    "oauth2": {
      "client_id": "1006117928571-lm402qdts3mm91v1bpl93gp2deakh4ag.apps.googleusercontent.com",
      "scopes": [
        "https://www.googleapis.com/auth/userinfo.email",
        "https://www.googleapis.com/auth/userinfo.profile"
      ]
    },
    "key": ""
  }