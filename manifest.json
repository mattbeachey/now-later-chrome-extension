
{
    "name": "Now and Later",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html",  
      "default_title": "Now & Later",
      "default_icon": "./assets/icon.png"
    },
    "background": {
      "page": "background.html"
    },
    "permissions": [
      "tabs"
    ]
  }